{
  "create": {
    "appName": "app",
    "path": {
      "type": "src/app/type",
      "typeIndex": "src/app/type.js",
      "module": "src/module"
    }
  },
  "build": {
    "file": "index.html",
    "output": "build"
  },
  "server": {
    "port": 8001,
    "ignore": [
      "lib/basisjs/demo",
      "lib/basisjs/docs",
      "lib/basisjs/test",
      "lib/basisjs/tour"
    ],
    "rewrite": {
      "^/api/v1/(.*)$": "http://plan.tpondemand.com/api/v1/$1 [P, QSA]",
      "^/slice/v1/matrix/(.*)$": "http://plan.tpondemand.com/slice/v1/matrix/$1 [P, QSA]",
      "^/storage/v1/(.*)$": "http://plan.tpondemand.com/storage/v1/$1 [P, QSA]"
    }
  }
}